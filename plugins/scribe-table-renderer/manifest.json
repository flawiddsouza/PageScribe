{
  "manifestVersion": 1,
  "name": "Scribe Table Renderer",
  "version": "1.0.0",
  "description": "A plugin that renders scribe table files",
  "contributes": [
    {
      "type": "renderer",
      "meta": {
        "type": "file",
        "renderer": "dist/table-renderer.js",
        "stylesheet": "dist/table-renderer.css",
        "fontHint": "text",
        "supportedExtensions": [
          ".stbl"
        ]
      }
    },
    {
      "type": "new_file",
      "meta": {
        "label": "New Scribe Table",
        "extension": ".stbl"
      }
    }
  ]
}
