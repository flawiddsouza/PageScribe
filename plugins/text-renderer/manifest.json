{
  "manifestVersion": 1,
  "name": "Text Renderer",
  "version": "1.0.0",
  "description": "A plugin that renders text",
  "contributes": [
    {
      "type": "renderer",
      "meta": {
        "type": "file",
        "renderer": "index.js",
        "fontHint": "text",
        "supportedExtensions": [
          ".txt"
        ]
      }
    }
  ]
}
