{
  "manifestVersion": 1,
  "name": "Code Renderer",
  "version": "1.0.0",
  "description": "A plugin that renders code",
  "contributes": [
    {
      "type": "renderer",
      "meta": {
        "type": "file",
        "renderer": "dist/code-renderer.js",
        "stylesheet": "dist/code-renderer.css",
        "fontHint": "code",
        "supportedExtensions": [
          ".js",
          ".ts",
          ".css",
          ".vue"
        ]
      }
    }
  ]
}
